{
  "id": null,
  "uid": "saque-pix-overview",
  "title": "Saque PIX - Observabilidade",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "id": 1,
      "title": "Throughput HTTP (req/s)",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[1m]))",
          "legendFormat": "Requests",
          "refId": "A"
        }
      ]
    },
    {
      "id": 2,
      "title": "Processamento de Saques (Δ 5m)",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "increase(withdraws_processed_total{status=\"success\"}[5m])",
          "legendFormat": "Sucesso",
          "refId": "A"
        },
        {
          "expr": "increase(withdraws_processed_total{status=\"error\"}[5m])",
          "legendFormat": "Erro",
          "refId": "B"
        }
      ]
    },
    {
      "id": 3,
      "title": "Tempo Médio de Processamento (últimos 5 min)",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 7,
        "w": 8,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "avg_over_time(withdraw_processing_time_seconds[5m])",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "vertical",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        }
      }
    },
    {
      "id": 4,
      "title": "Emails Enviados vs Erros (Δ 5m)",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 7,
        "w": 8,
        "x": 8,
        "y": 8
      },
      "targets": [
        {
          "expr": "increase(emails_sent_total{status=\"success\"}[5m])",
          "legendFormat": "Sucesso",
          "refId": "A"
        },
        {
          "expr": "increase(emails_sent_total{status=\"error\"}[5m])",
          "legendFormat": "Erro",
          "refId": "B"
        }
      ]
    },
    {
      "id": 5,
      "title": "Alertas de Saldo Insuficiente (Δ 5m)",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 7,
        "w": 8,
        "x": 16,
        "y": 8
      },
      "targets": [
        {
          "expr": "increase(insufficient_balance_total[5m])",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "vertical",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        }
      }
    }
  ],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-15m",
    "to": "now"
  }
}

